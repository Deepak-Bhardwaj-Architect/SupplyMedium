<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>CheckLogin</servlet-name>
        <servlet-class>supply.medium.marketing.servlet.CheckLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddMarketingPerson</servlet-name>
        <servlet-class>supply.medium.marketing.servlet.AddMarketingPerson</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CheckLogin</servlet-name>
        <url-pattern>/CheckLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddMarketingPerson</servlet-name>
        <url-pattern>/AddMarketingPerson</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
